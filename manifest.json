{
  "manifest_version": 3,
  "name": "Nice Assistant",
  "description": "Nice popout window extension",
  "version": "1.0",
  "action": {
    "default_popup": "NiceAssistant.html",
    "default_icon": "NiceAssistantIcon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}